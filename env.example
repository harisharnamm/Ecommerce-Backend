# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Security
JWT_SECRET=your-jwt-secret-here
COOKIE_SECRET=your-cookie-secret-here

# Server Configuration
PORT=9000
NODE_ENV=development

# Medusa Configuration
MEDUSA_ADMIN_ONBOARDING_TYPE=default

# CORS Configuration (for production)
STORE_CORS=http://localhost:8000,https://yourdomain.com
ADMIN_CORS=http://localhost:5173,http://localhost:9000,https://yourdomain.com
AUTH_CORS=http://localhost:5173,http://localhost:9000,https://yourdomain.com

# Redis (optional, for production)
REDIS_URL=redis://localhost:6379

# File Storage (optional)
FILE_SERVICE_URL=http://localhost:9000
FILE_SERVICE_SECRET=your-file-service-secret
